<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:context=".Live"
    tools:openDrawer="end">

    <com.google.android.material.navigation.NavigationView
        android:id="@+id/nav_view"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        app:headerLayout="@layout/header"
        app:menu="@menu/main_menu" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="453dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal"
            android:background="#67bef5"
            tools:layout_editor_absoluteX="85dp"
            tools:layout_editor_absoluteY="453dp">

            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbar"
                android:layout_width="46dp"
                android:layout_height="42dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:minHeight="?attr/actionBarSize"
                android:theme="?attr/actionBarTheme"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

            </androidx.appcompat.widget.Toolbar>

            <TextView
                android:id="@+id/currentUser"
                android:layout_width="100dp"
                android:layout_height="34dp"
                android:layout_marginTop="12dp"
                android:cursorVisible="false"
                android:gravity="center"
                android:text="TextView"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="99dp"
                android:layout_height="34dp"
                android:layout_marginTop="12dp"
                android:gravity="center"
                android:text="Current Status :"
                android:textStyle="italic" />


            <pl.droidsonroids.gif.GifImageView
                android:id="@+id/currentStatusGif"
                android:layout_marginTop="12dp"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:visibility="visible" />

            <TextView
                android:id="@+id/currentStatusView"
                android:layout_width="102dp"
                android:layout_height="34dp"
                android:layout_marginTop="12dp"
                android:backgroundTint="#ece3ff"
                android:gravity="center"
                android:text="" />
        </LinearLayout>

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/linearLayout4"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#F0F8FF"
                android:theme="@style/MyMaterialTheme"
                tools:context="com.example.bondhu.Users"
                tools:viewBindingIgnore="true">


                <TextView
                    android:id="@+id/noUsersText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/no_users_found"
                    android:visibility="gone"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />


                <pl.droidsonroids.gif.GifImageView
                    android:id="@+id/friend1Gif"
                    android:layout_width="52dp"
                    android:layout_height="55dp"
                    android:visibility="invisible"
                    app:layout_constraintBottom_toTopOf="@+id/friend1"
                    app:layout_constraintEnd_toEndOf="@+id/friend1"
                    app:layout_constraintHorizontal_bias="0.481"
                    app:layout_constraintStart_toStartOf="@+id/friend1" />

                <pl.droidsonroids.gif.GifImageView
                    android:id="@+id/friend2Gif"
                    android:layout_width="59dp"
                    android:layout_height="56dp"
                    android:visibility="invisible"
                    app:layout_constraintBottom_toTopOf="@+id/friend2"
                    app:layout_constraintEnd_toEndOf="@+id/friend2"
                    app:layout_constraintHorizontal_bias="0.431"
                    app:layout_constraintStart_toStartOf="@+id/friend2" />

                <pl.droidsonroids.gif.GifImageView
                    android:id="@+id/friend5Gif"
                    android:layout_width="71dp"
                    android:layout_height="63dp"
                    android:visibility="invisible"
                    app:layout_constraintBottom_toTopOf="@+id/friend5"
                    app:layout_constraintEnd_toEndOf="@+id/friend5"
                    app:layout_constraintStart_toStartOf="@+id/friend5" />

                <pl.droidsonroids.gif.GifImageView
                    android:id="@+id/friend6Gif"
                    android:layout_width="77dp"
                    android:layout_height="61dp"
                    android:visibility="invisible"
                    app:layout_constraintBottom_toTopOf="@+id/friend6"
                    app:layout_constraintEnd_toEndOf="@+id/friend6"
                    app:layout_constraintStart_toStartOf="@+id/friend6" />

                <pl.droidsonroids.gif.GifImageView
                    android:id="@+id/friend4Gif"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:visibility="invisible"
                    app:layout_constraintBottom_toTopOf="@+id/friend4"
                    app:layout_constraintEnd_toEndOf="@+id/friend4"
                    app:layout_constraintHorizontal_bias="0.468"
                    app:layout_constraintStart_toStartOf="@+id/friend4" />

                <pl.droidsonroids.gif.GifImageView
                    android:id="@+id/friend3Gif"
                    android:layout_width="61dp"
                    android:layout_height="56dp"
                    android:visibility="invisible"
                    app:layout_constraintBottom_toTopOf="@+id/friend3"
                    app:layout_constraintEnd_toEndOf="@+id/friend3"
                    app:layout_constraintHorizontal_bias="0.535"
                    app:layout_constraintStart_toStartOf="@+id/friend3" />

                <EditText
                    android:id="@+id/etLiveStatus"
                    android:layout_width="83dp"
                    android:layout_height="35dp"
                    android:layout_marginTop="16dp"
                    android:hint="add new Status!"
                    android:text=""
                    android:textSize="10sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.042"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />


                <Spinner
                    android:id="@+id/spinnerStatus"
                    android:layout_width="150dp"
                    android:layout_height="35dp"
                    android:layout_marginTop="16dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.601"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <Button
                    android:id="@+id/btnLiveStatusSelectAdd"
                    android:layout_width="40dp"
                    android:layout_height="35dp"
                    android:background="#67bef5"
                    android:text="update"
                    android:textColor="#ffffff"
                    android:textSize="8sp"

                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/spinnerStatus"
                    app:layout_constraintStart_toEndOf="@+id/spinnerStatus" />


                <Button
                    android:id="@+id/btnLiveStatusData2"
                    android:layout_width="40dp"
                    android:layout_height="35dp"
                    android:layout_marginStart="8dp"
                    android:background="#67bef5"
                    android:text="add Status"
                    android:textColor="#ffffff"
                    android:textSize="8sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/etLiveStatus"
                    app:layout_constraintStart_toEndOf="@+id/etLiveStatus" />

                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:id="@+id/btncurrentUser"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="304dp"
                    android:clickable="true"
                    android:focusable="true"
                    android:gravity="center_vertical"
                    android:textAlignment="center"
                    android:textColor="#ffffff"
                    app:backgroundTint="#eb5834"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.501"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@drawable/connect_icon"
                    app:strokeColor="#ffffff"
                    app:strokeWidth="1dp"
                    tools:ignore="RtlCompat" />



                <ListView
                    android:id="@+id/totalStatusList"
                    android:layout_width="108dp"
                    android:layout_height="156dp"
                    android:layout_marginBottom="56dp"
                    android:visibility="invisible"
                    app:layout_constraintBottom_toTopOf="@+id/btncurrentUser"
                    app:layout_constraintEnd_toEndOf="@+id/btncurrentUser"
                    app:layout_constraintHorizontal_bias="0.507"
                    app:layout_constraintStart_toStartOf="@+id/btncurrentUser">

                </ListView>

                <Button
                    android:id="@+id/friend1"
                    android:layout_width="wrap_content"
                    android:layout_height="25dp"
                    android:layout_marginEnd="40dp"
                    android:layout_marginBottom="84dp"
                    android:background="@drawable/mybutton"
                    android:clickable="true"
                    android:gravity="center"
                    android:textAppearance="?attr/buttonStyleSmall"
                    android:textSize="10dp"
                    android:visibility="invisible"
                    app:backgroundTint="@color/base_blue_button"
                    app:layout_constraintBottom_toBottomOf="@+id/btncurrentUser"
                    app:layout_constraintEnd_toStartOf="@+id/btncurrentUser" />

                <Button
                    android:id="@+id/friend2"
                    android:layout_width="wrap_content"
                    android:layout_height="25dp"
                    android:layout_marginTop="56dp"
                    android:layout_marginEnd="60dp"
                    android:background="@drawable/mybutton"
                    android:clickable="true"
                    android:gravity="center"
                    android:textAppearance="?attr/buttonStyleSmall"
                    android:textSize="10dp"
                    android:visibility="invisible"
                    app:backgroundTint="@color/base_blue_button"
                    app:layout_constraintEnd_toStartOf="@+id/btncurrentUser"
                    app:layout_constraintTop_toTopOf="@+id/btncurrentUser" />

                <Button

                    android:id="@+id/friend3"
                    android:layout_width="wrap_content"
                    android:layout_height="25dp"
                    android:layout_marginStart="40dp"
                    android:layout_marginBottom="84dp"
                    android:background="@drawable/mybutton"
                    android:clickable="true"
                    android:gravity="center"
                    android:textAppearance="?attr/buttonStyleSmall"
                    android:textSize="10dp"
                    android:visibility="invisible"
                    app:backgroundTint="@color/base_blue_button"
                    app:layout_constraintBottom_toBottomOf="@+id/btncurrentUser"
                    app:layout_constraintStart_toEndOf="@+id/btncurrentUser" />

                <Button

                    android:id="@+id/friend4"
                    android:layout_width="wrap_content"
                    android:layout_height="25dp"
                    android:layout_marginStart="60dp"
                    android:layout_marginTop="64dp"
                    android:background="@drawable/mybutton"
                    android:clickable="true"
                    android:gravity="center"
                    android:textAppearance="?attr/buttonStyleSmall"
                    android:textSize="10dp"
                    android:visibility="invisible"
                    app:backgroundTint="@color/base_blue_button"
                    app:layout_constraintStart_toEndOf="@+id/btncurrentUser"
                    app:layout_constraintTop_toTopOf="@+id/btncurrentUser" />

                <Button

                    android:id="@+id/friend5"
                    android:layout_width="wrap_content"
                    android:layout_height="25dp"
                    android:layout_marginTop="156dp"
                    android:layout_marginEnd="64dp"
                    android:background="@drawable/mybutton"
                    android:clickable="true"
                    android:gravity="center"
                    android:textAppearance="?attr/buttonStyleSmall"
                    android:textSize="10dp"
                    android:visibility="invisible"
                    app:backgroundTint="@color/base_blue_button"
                    app:layout_constraintEnd_toEndOf="@+id/btncurrentUser"
                    app:layout_constraintTop_toTopOf="@+id/btncurrentUser" />


                <Button

                    android:id="@+id/friend6"
                    android:layout_width="wrap_content"
                    android:layout_height="25dp"
                    android:layout_marginStart="28dp"
                    android:layout_marginTop="100dp"
                    android:background="@drawable/mybutton"
                    android:clickable="true"
                    android:gravity="center"
                    android:textAppearance="?attr/buttonStyleSmall"
                    android:textSize="10dp"
                    android:visibility="invisible"
                    app:backgroundTint="@color/base_blue_button"
                    app:layout_constraintStart_toStartOf="@+id/btncurrentUser"
                    app:layout_constraintTop_toBottomOf="@+id/btncurrentUser" />


                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:id="@+id/fabCheckScore"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="116dp"
                    android:clickable="true"
                    app:fabSize="mini"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.997"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@android:drawable/stat_notify_sync" />

                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:id="@+id/updateScore"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:backgroundTint="#faf4b1"
                    android:clickable="true"
                    android:focusable="true"
                    app:fabSize="mini"
                    android:visibility="invisible"
                    app:layout_constraintBottom_toTopOf="@+id/fabCheckScore"
                    app:layout_constraintEnd_toEndOf="@+id/fabCheckScore"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="@+id/fabCheckScore"
                    app:rippleColor="@android:color/white" />

                <ProgressBar
                    android:id="@+id/progressBar"
                    style="@android:style/Widget.ProgressBar.Horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="10dp"
                    android:indeterminate="false"
                    android:visibility="invisible"
                    app:layout_constraintEnd_toEndOf="@+id/friend1"
                    app:layout_constraintStart_toStartOf="@+id/friend1"
                    app:layout_constraintTop_toBottomOf="@+id/friend1" />

                <ProgressBar
                    android:id="@+id/progressBar2"
                    style="@android:style/Widget.ProgressBar.Horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="10dp"
                    android:indeterminate="false"
                    android:visibility="invisible"
                    app:layout_constraintEnd_toEndOf="@+id/friend2"
                    app:layout_constraintStart_toStartOf="@+id/friend2"
                    app:layout_constraintTop_toBottomOf="@+id/friend2" />

                <ProgressBar
                    android:id="@+id/progressBar3"
                    style="@android:style/Widget.ProgressBar.Horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="10dp"
                    android:indeterminate="false"
                    android:visibility="invisible"
                    app:layout_constraintEnd_toEndOf="@+id/friend3"
                    app:layout_constraintStart_toStartOf="@+id/friend3"
                    app:layout_constraintTop_toBottomOf="@+id/friend3" />

                <ProgressBar
                    android:id="@+id/progressBar4"
                    style="@android:style/Widget.ProgressBar.Horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="10dp"
                    android:indeterminate="false"
                    android:visibility="invisible"
                    app:layout_constraintEnd_toEndOf="@+id/friend4"
                    app:layout_constraintStart_toStartOf="@+id/friend4"
                    app:layout_constraintTop_toBottomOf="@+id/friend4" />

                <ProgressBar
                    android:id="@+id/progressBar5"
                    style="@android:style/Widget.ProgressBar.Horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="10dp"
                    android:indeterminate="false"
                    android:visibility="invisible"
                    app:layout_constraintEnd_toEndOf="@+id/friend5"
                    app:layout_constraintStart_toStartOf="@+id/friend5"
                    app:layout_constraintTop_toBottomOf="@+id/friend5" />

                <ProgressBar
                    android:id="@+id/progressBar6"
                    style="@android:style/Widget.ProgressBar.Horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="10dp"
                    android:indeterminate="false"
                    android:visibility="invisible"
                    app:layout_constraintEnd_toEndOf="@+id/friend6"
                    app:layout_constraintStart_toStartOf="@+id/friend6"
                    app:layout_constraintTop_toBottomOf="@+id/friend6" />

                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:id="@+id/fabStatusLevel1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="608dp"
                    android:clickable="true"
                    android:contentDescription="fabButton"
                    android:focusable="true"
                    android:gravity="center_vertical"
                    app:backgroundTint="#1b5285"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.478"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:maxImageSize="56dp"
                    app:srcCompat="@drawable/panda_paw_icon"
                    app:strokeColor="#ffffff"
                    app:strokeWidth="1dp"
                    app:tint="@null"
                    tools:ignore="RtlCompat" />

                <com.nightonke.boommenu.BoomMenuButton
                    android:id="@+id/bmb1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="32dp"
                    android:layout_marginBottom="24dp"
                    app:bmb_buttonRadius="20dp"
                    app:bmb_highlightedColor="@android:color/holo_blue_dark"
                    app:bmb_normalColor="@color/dark_blue_panda"
                    app:bmb_rippleEffect="true"
                    app:bmb_unableColor="@android:color/white"
                    app:bmb_dotRadius="0dp"
                    app:layout_constraintBottom_toBottomOf="@+id/fabStatusLevel1"
                    app:layout_constraintEnd_toStartOf="@+id/fabStatusLevel1" >
                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        app:srcCompat="@drawable/happy_icon"
                        android:layout_gravity="center"/>

                    <TextView
                        android:layout_width="40dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="15dp"
                        android:gravity="center"
                        android:text="Positive"
                        android:textSize="5sp"
                        android:textColor="@android:color/white"/>

                </com.nightonke.boommenu.BoomMenuButton>
                <com.nightonke.boommenu.BoomMenuButton
                    android:id="@+id/bmb2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="73dp"
                    app:bmb_buttonRadius="20dp"
                    app:bmb_highlightedColor="@android:color/holo_blue_dark"
                    app:bmb_normalColor="@color/dark_blue_panda"
                    app:bmb_rippleEffect="true"
                    app:bmb_unableColor="@android:color/white"
                    app:bmb_dotRadius="0dp"
                    app:layout_constraintBottom_toBottomOf="@+id/fabStatusLevel1"
                    app:layout_constraintEnd_toStartOf="@+id/fabStatusLevel1" >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        app:srcCompat="@drawable/sad_icon" />

                    <TextView
                        android:layout_width="40dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="15dp"
                        android:gravity="center"
                        android:text="Negative"
                        android:textSize="5sp"
                        android:textColor="@android:color/white"/>

                </com.nightonke.boommenu.BoomMenuButton>

                <com.nightonke.boommenu.BoomMenuButton
                    android:id="@+id/bmb3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="28dp"
                    app:bmb_buttonRadius="20dp"
                    app:bmb_highlightedColor="@android:color/holo_blue_dark"
                    app:bmb_normalColor="@color/dark_blue_panda"
                    app:bmb_rippleEffect="true"
                    app:bmb_unableColor="@android:color/white"
                    app:bmb_dotRadius="0dp"
                    app:layout_constraintBottom_toTopOf="@+id/fabStatusLevel1"
                    app:layout_constraintEnd_toEndOf="@+id/fabStatusLevel1"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="@+id/fabStatusLevel1" >
                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        app:srcCompat="@drawable/most_usage_icon"
                        android:layout_gravity="center"/>

                    <TextView
                        android:layout_width="40dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="15dp"
                        android:gravity="center"
                        android:text="Commonly Used"
                        android:textSize="5sp"
                        android:textColor="@android:color/white"/>

                </com.nightonke.boommenu.BoomMenuButton>

                <com.nightonke.boommenu.BoomMenuButton
                    android:id="@+id/bmb4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="72dp"
                    app:bmb_buttonRadius="20dp"
                    app:bmb_highlightedColor="@android:color/holo_blue_dark"
                    app:bmb_normalColor="@color/dark_blue_panda"
                    app:bmb_rippleEffect="true"
                    app:bmb_unableColor="@android:color/white"
                    app:bmb_dotRadius="0dp"
                    app:layout_constraintBottom_toBottomOf="@+id/fabStatusLevel1"
                    app:layout_constraintStart_toEndOf="@+id/fabStatusLevel1" >
                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        app:srcCompat="@drawable/normal_icon"
                        android:layout_gravity="center"/>

                    <TextView
                        android:layout_width="40dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="15dp"
                        android:gravity="center"
                        android:text="Normal"
                        android:textSize="5sp"
                        android:textColor="@android:color/white"/>

                </com.nightonke.boommenu.BoomMenuButton>
                <com.nightonke.boommenu.BoomMenuButton
                    android:id="@+id/bmb5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="32dp"
                    android:layout_marginBottom="24dp"
                    app:bmb_buttonRadius="20dp"
                    app:bmb_highlightedColor="@android:color/holo_blue_dark"
                    app:bmb_normalColor="@color/dark_blue_panda"
                    app:bmb_rippleEffect="true"
                    app:bmb_unableColor="@android:color/white"
                    app:bmb_dotRadius="0dp"
                    app:layout_constraintBottom_toBottomOf="@+id/fabStatusLevel1"
                    app:layout_constraintStart_toEndOf="@+id/fabStatusLevel1" >
                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        app:srcCompat="@drawable/panda_paw_icon"
                        app:tint="@android:color/white"
                        android:layout_gravity="center"/>

                    <TextView
                        android:layout_width="40dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="15dp"
                        android:gravity="center"
                        android:text="Activity"
                        android:textSize="5sp"
                        android:textColor="@android:color/white"/>

                </com.nightonke.boommenu.BoomMenuButton>


            </androidx.constraintlayout.widget.ConstraintLayout>

        </ScrollView>
    </LinearLayout>
</androidx.drawerlayout.widget.DrawerLayout>